{"ast":null,"code":"import ConnexionComponent from '@/components/ConnexionComponent.vue';\nimport InscriptionComponent from '@/components/InscriptionComponent.vue';\nimport defineComponent from 'vue';\nimport useConfirm from 'primevue/useconfirm';\nexport default {\n  inject: ['loggedin'],\n  components: {\n    ConnexionComponent,\n    InscriptionComponent\n  },\n\n  data() {\n    return {\n      alreadyUser: false\n    };\n  },\n\n  methods: {\n    changeForm(event) {\n      this.alreadyUser = !this.alreadyUser;\n      this.loggedin = true;\n      console.log(this.loggedin);\n\n      this.$confirm.require({\n        target: event.currentTarget,\n        message: 'Are you sure you want to proceed ?',\n        icon: 'pi pi-exclamation-triangle',\n        accept: () => {\n          console.log(\"Accepted\");\n        },\n        reject: () => {\n          console.log(\"Rejected\");\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsBA,OAAOA,kBAAP,MAA+B,qCAA/B;AACA,OAAOC,oBAAP,MAAiC,uCAAjC;AACA,OAAOC,eAAP,MAA4B,KAA5B;AACA,OAAOC,UAAP,MAAuB,qBAAvB;AAEA,eAAe;AACbC,QAAM,EAAE,CAAC,UAAD,CADK;AAEbC,YAAU,EAAE;AACVL,sBADU;AAEVC;AAFU,GAFC;;AAMbK,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE;AADR,KAAP;AAGD,GAVY;;AAWbC,SAAO,EAAE;AACPC,cAAU,CAACC,KAAD,EAAQ;AAChB,WAAKH,WAAL,GAAmB,CAAC,KAAKA,WAAzB;AACA,WAAKI,QAAL,GAAgB,IAAhB;AACAC,aAAO,CAACC,GAAR,CAAY,KAAKF,QAAjB;;AACA,WAAKG,QAAL,CAAcC,OAAd,CAAsB;AACpBC,cAAM,EAAEN,KAAK,CAACO,aADM;AAEpBC,eAAO,EAAE,oCAFW;AAGpBC,YAAI,EAAE,4BAHc;AAIpBC,cAAM,EAAE,MAAM;AACVR,iBAAO,CAACC,GAAR,CAAY,UAAZ;AACH,SANmB;AAOpBQ,cAAM,EAAE,MAAM;AACVT,iBAAO,CAACC,GAAR,CAAY,UAAZ;AACJ;AAToB,OAAtB;AAWF;;AAhBO;AAXI,CAAf","names":["ConnexionComponent","InscriptionComponent","defineComponent","useConfirm","inject","components","data","alreadyUser","methods","changeForm","event","loggedin","console","log","$confirm","require","target","currentTarget","message","icon","accept","reject"],"sourceRoot":"","sources":["/home/eisti/Desktop/ING3/MicroServices/projet_wom/project-wom/vue_js/src/views/ConnexionInscription.vue"],"sourcesContent":["<template>\n  <div id=\"connexionInscription\">\n    <div v-if=\"alreadyUser\" id=\"connexion\">\n      <ConnexionComponent/>\n      <button v-on:click=\"changeForm()\">Pas encore inscrit ?</button>\n    </div>\n    <div v-if=\"!alreadyUser\" id=\"connexioninscription\">\n      <InscriptionComponent/>\n      <ConfirmPopup>\n          <template #message=\"slotProps\">\n              <div class=\"flex p-4\">\n                  <i :class=\"slotProps.message.icon\" style=\"font-size: 1.5rem\"></i>\n                  <p class=\"pl-2\">{{ slotProps.message.message }}</p>\n              </div>\n          </template>\n      </ConfirmPopup>\n      <Button @click=\"changeForm($event)\" icon=\"pi pi-check\">Déjà inscrit ?</Button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ConnexionComponent from '@/components/ConnexionComponent.vue'\nimport InscriptionComponent from '@/components/InscriptionComponent.vue'\nimport defineComponent from 'vue'\nimport useConfirm from 'primevue/useconfirm'\n\nexport default {\n  inject: ['loggedin'],\n  components: {\n    ConnexionComponent,\n    InscriptionComponent\n  },\n  data() {\n    return {\n      alreadyUser: false\n    }\n  },\n  methods: {\n    changeForm(event) {\n      this.alreadyUser = !this.alreadyUser\n      this.loggedin = true\n      console.log(this.loggedin)\n      this.$confirm.require({\n        target: event.currentTarget,\n        message: 'Are you sure you want to proceed ?',\n        icon: 'pi pi-exclamation-triangle',\n        accept: () => {\n            console.log(\"Accepted\")\n        },\n        reject: () => {\n            console.log(\"Rejected\")\n        }\n      })\n    }\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}